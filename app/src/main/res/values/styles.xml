<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppCompatEditTextMine" parent="ThemeOverlay.MaterialComponents.TextInputEditText">
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="TextViewMiddle" parent="Widget.AppCompat.TextView">
        <item name="android:textSize">@dimen/text_middle</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="TextViewLarge" parent="Widget.AppCompat.TextView">
        <item name="android:textSize">@dimen/text_large</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="TextViewSmall" parent="Widget.AppCompat.TextView">
        <item name="android:textSize">@dimen/text_small</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="ShapeAppearance.Card.All" parent="ShapeAppearance.MaterialComponents">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">12dp</item>
        <item name="cornerSizeTopRight">12dp</item>
        <item name="cornerSizeBottomRight">12dp</item>
        <item name="cornerSizeBottomLeft">12dp</item>
    </style>

</resources>